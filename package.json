{
  "name": "recipes",
  "private": true,
  "description": "A simple recipe book written using Gatsby",
  "version": "0.1.0",
  "author": "Mike Ratcliffe <michael@ratcliffefamily.org>",
  "bugs": {
    "url": "https://github.com/MikeRatcliffe/NewRecipes/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "bootstrap": "5.3.0-alpha3",
    "gatsby": "^5.8.1",
    "gatsby-plugin-flexsearch": "^1.0.3",
    "gatsby-plugin-image": "^3.8.0",
    "gatsby-plugin-manifest": "^5.8.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sass": "^6.8.0",
    "gatsby-plugin-sharp": "^5.8.1",
    "gatsby-plugin-sitemap": "^6.8.0",
    "gatsby-plugin-typescript": "^5.8.0",
    "gatsby-plugin-typography": "^5.8.0",
    "gatsby-remark-copy-linked-files": "^6.8.0",
    "gatsby-remark-footnotes": "^0.0.8",
    "gatsby-remark-images": "^7.8.0",
    "gatsby-remark-smartypants": "^6.8.0",
    "gatsby-source-filesystem": "^5.8.0",
    "gatsby-transformer-remark": "^6.8.0",
    "gatsby-transformer-sharp": "^5.8.0",
    "node-html-parser": "^6.1.5",
    "node-sass": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "slug": "^8.2.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.4",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.5.0",
    "babel-preset-gatsby": "^3.8.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "gatsby-plugin-pnpm-gatsby-5": "^1.2.11",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^15.0.0",
    "path": "^0.12.7",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "url-join": "^5.0.0"
  },
  "homepage": "https://github.com/MikeRatcliffe/NewRecipes",
  "keywords": [
    "recipes"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeRatcliffe/NewRecipes"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18"
      }
    },
    "overrides": {
      "eslint": "8.37.0",
      "eslint-config-react-app": "7.0.1",
      "eslint-plugin-flowtype": "8.0.3",
      "mdast-util-to-hast": "7.0.0",
      "@npmcli/move-file": "3.0.0",
      "source-map-resolve": "0.6.0",
      "svgo": "3.0.2",
      "@typescript-eslint/eslint-plugin": "^5.57.0",
      "@typescript-eslint/parser": "5.57.0"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "build:serve": "gatsby build && gatsby serve -p 8000",
    "clobber": "rm -rf .cache public node_modules package-lock.json",
    "clobber:install": "npm run clobber && npm install",
    "clobber:install:build": "npm run clobber && npm install && gatsby build",
    "clobber:install:build:serve": "npm clobber && npm install && gatsby build && gatsby serve -p 8000",
    "clobber:install:develop": "npm run clobber && npm install && gatsby develop",
    "develop": "gatsby develop",
    "lint": "npm run lint:eslint; npm run lint:ts; npm run lint:prettier",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:eslint:ci": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --max-warnings 0",
    "lint:eslint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --fix",
    "lint:prettier": "prettier --list-different \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:ts": "tsc --noEmit",
    "serve": "gatsby serve -p 8000",
    "test:coverage": "jest --ci --coverage",
    "test": "jest"
  }
}
