{
  "name": "recipes",
  "private": true,
  "description": "A simple recipe book written using Gatsby",
  "version": "0.1.0",
  "author": "Mike Ratcliffe <michael@ratcliffefamily.org>",
  "bugs": {
    "url": "https://github.com/MikeRatcliffe/NewRecipes/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "bootstrap": "5.3.2",
    "gatsby": "^5.12.9",
    "gatsby-plugin-flexsearch": "^1.0.3",
    "gatsby-plugin-image": "^3.12.3",
    "gatsby-plugin-manifest": "^5.12.3",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sass": "^6.12.3",
    "gatsby-plugin-sharp": "^5.12.3",
    "gatsby-plugin-sitemap": "^6.12.3",
    "gatsby-plugin-typescript": "^5.12.1",
    "gatsby-plugin-typography": "^5.12.0",
    "gatsby-remark-copy-linked-files": "^6.12.0",
    "gatsby-remark-footnotes": "^0.0.8",
    "gatsby-remark-images": "^7.12.3",
    "gatsby-remark-smartypants": "^6.12.0",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-remark": "^6.12.3",
    "gatsby-transformer-sharp": "^5.12.3",
    "node-html-parser": "^6.1.11",
    "node-sass": "^9.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "slug": "^8.2.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/prop-types": "^15.7.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.12.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "url-join": "^5.0.0"
  },
  "homepage": "https://github.com/MikeRatcliffe/NewRecipes",
  "keywords": [
    "recipes"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeRatcliffe/NewRecipes"
  },
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "build:serve": "gatsby build && gatsby serve -p 8000",
    "clobber": "rm -rf .cache public node_modules package-lock.json",
    "clobber:install": "npm run clobber && npm install",
    "clobber:install:build": "npm run clobber && npm install && gatsby build",
    "clobber:install:build:serve": "npm clobber && npm install && gatsby build && gatsby serve -p 8000",
    "clobber:install:develop": "npm run clobber && npm install && gatsby develop",
    "develop": "gatsby develop",
    "lint": "npm run lint:eslint; npm run lint:ts; npm run lint:prettier",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:eslint:ci": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --max-warnings 0",
    "lint:eslint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --fix",
    "lint:prettier": "prettier --list-different \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:ts": "tsc --noEmit",
    "serve": "gatsby serve -p 8000",
    "test:coverage": "jest --ci --coverage",
    "test": "jest"
  }
}
